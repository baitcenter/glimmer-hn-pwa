{
  "hosting": {
    "public": "dist",
    "headers": [
      {
        "source": "/",
        "headers": [
          {
            "key": "Link",
            "value": "</app-31b3e25a23e98bcfdccadcf3d76335e7.js>;rel=preload;as=script"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=30672000"
          }
        ]
      },
      {
        "source": "**/*.@(json|js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=86400"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}