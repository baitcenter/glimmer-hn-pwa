<li class="news-item fadein">
  <h2>ABOUT {{@result.id}}</h2>
  <div class="person">
    <div class="person--avatar">
      {{#if @result.avg}}
        <img src={{@result.avg}}>
      {{else}}
        <img
          src='data:image/png;base64,'/>
      {{/if}}
      <br>
      <span class="karma">Karma: {{@result.karma}}</span>
    </div>
    <div class="person--info">
      <h4>ID: {{@result.id}}</h4>
      <h4>created: {{@result.id}}</h4>
      {{{@result.about}}}
    </div>
  </div>
</li>