<header class="header">
  <nav class="inner">
    <a href="/">
      <img
        alt="Glimmer HN PWA"
        class="logo"
        src='data:image/png;base64,'/>
      Glimmer.js |
    </a>
    <a href="#/" class="{{if (eq routeMode 'top') "selected"}} {{if (eq routeMode '') "selected"}}">Top</a>
    <a href="#/newest" class="{{if (eq routeMode 'newest') "selected"}}">New</a>
    <a href="#/show" class="{{if (eq routeMode 'show') "selected"}}">Show</a>
    <a href="#/ask" class="{{if (eq routeMode 'ask') "selected"}}">Ask</a>
    <a href="#/jobs" class="{{if (eq routeMode 'jobs') "selected"}}">Jobs</a>
  </nav>
</header>

<div class="news-view">
  <div class="news-list-nav">
    <!--<a onclick={{action previousPage}}>&lt; prev</a>-->
    <!--<a onclick={{action nextPage}}>more &gt;</a>-->
  </div>
  <div class="news-list">
    <div class="view">
      <items-list
        @results={{results}}
          @onDestroy={{action cleanUp}}>
      </items-list>
    </div>
  </div>
</div>